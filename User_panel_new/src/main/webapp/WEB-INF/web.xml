<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ForgotPassword</servlet-name>
		<display-name>ForgotPassword</display-name>
		<description></description>
		<servlet-class>com.codeo.shop.controller.ForgotPassword</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>OrderCreation</servlet-name>
		<display-name>OrderCreation</display-name>
		<description></description>
		<servlet-class>com.codeo.shop.controller.OrderCreation</servlet-class>
	</servlet>
		<servlet-mapping>
		<servlet-name>ForgotPassword</servlet-name>
		<url-pattern>/ForgotPassword</url-pattern>
	</servlet-mapping>
		<servlet-mapping>
			<servlet-name>OrderCreation</servlet-name>
			<url-pattern>/OrderCreation</url-pattern>
		</servlet-mapping>


</web-app>
